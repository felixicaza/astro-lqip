{
  "name": "astro-lqip",
  "version": "1.0.0",
  "description": "Native extended Astro component for generating low quality image placeholders (LQIP).",
  "keywords": [
    "astro",
    "astro-component",
    "astro-integration",
    "image",
    "lqip"
  ],
  "homepage": "https://github.com/felixicaza/astro-lqip",
  "bugs": {
    "url": "https://github.com/felixicaza/astro-lqip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixicaza/astro-lqip.git"
  },
  "license": "MIT",
  "author": "Felix Icaza",
  "type": "module",
  "exports": {
    "./components": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "pnpm -r preview",
    "release": "bumpp -r"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
    "lqip-modern": "2.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.30.0",
    "@typescript-eslint/parser": "8.35.0",
    "astro": "5.10.1",
    "bumpp": "10.2.0",
    "eslint": "9.30.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.42.0",
    "eslint-plugin-yml": "1.18.0",
    "globals": "16.2.0",
    "jiti": "2.4.2",
    "nano-staged": "0.8.0",
    "neostandard": "0.12.1",
    "simple-git-hooks": "2.13.0"
  },
  "peerDependencies": {
    "astro": ">=3.3.0"
  }
}
