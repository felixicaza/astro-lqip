{
  "name": "astro-lqip",
  "version": "1.3.2",
  "description": "Native extended Astro components for generating low quality image placeholders (LQIP).",
  "keywords": [
    "astro",
    "astro-component",
    "astro-integration",
    "image",
    "picture",
    "lqip"
  ],
  "homepage": "https://astro-lqip.web.app",
  "bugs": {
    "url": "https://github.com/felixicaza/astro-lqip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixicaza/astro-lqip.git"
  },
  "license": "MIT",
  "author": "Felix Icaza",
  "type": "module",
  "exports": {
    "./components": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "pnpm -r preview",
    "release": "bumpp -r"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
    "plaiceholder": "3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@typescript-eslint/parser": "8.44.0",
    "astro": "5.13.8",
    "bumpp": "10.2.3",
    "eslint": "9.35.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsonc": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.56.3",
    "eslint-plugin-yml": "1.18.0",
    "globals": "16.4.0",
    "jiti": "2.5.1",
    "nano-staged": "0.8.0",
    "neostandard": "0.12.2",
    "simple-git-hooks": "2.13.1"
  },
  "peerDependencies": {
    "astro": ">=3.3.0"
  }
}
