{
  "name": "astro-lqip",
  "version": "1.7.2",
  "description": "Native extended Astro components for generating low quality image placeholders (LQIP).",
  "keywords": [
    "astro",
    "astro-component",
    "astro-integration",
    "image",
    "picture",
    "lqip"
  ],
  "homepage": "https://astro-lqip.web.app",
  "bugs": {
    "url": "https://github.com/felixicaza/astro-lqip/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixicaza/astro-lqip.git"
  },
  "license": "MIT",
  "author": "Felix Icaza",
  "type": "module",
  "exports": {
    "./components": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build:docs": "pnpm --filter ./docs -r build",
    "dev": "pnpm --filter=!./docs -r dev",
    "dev:docs": "pnpm --filter ./docs -r dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "pnpm -r preview",
    "release": "bumpp",
    "test:fixtures": "pnpm --filter \"./tests/fixtures/**\" -r build",
    "test:fixtures:preview": "pnpm --filter \"./tests/fixtures/**\" -r preview"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*": "pnpm lint:fix"
  },
  "dependencies": {
    "plaiceholder": "3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@typescript-eslint/parser": "8.53.0",
    "astro": "5.16.9",
    "bumpp": "10.4.0",
    "eslint": "9.39.2",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-jsonc": "2.21.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.88.1",
    "eslint-plugin-yml": "1.19.1",
    "globals": "17.0.0",
    "jiti": "2.6.1",
    "nano-staged": "0.9.0",
    "neostandard": "0.12.2",
    "simple-git-hooks": "2.13.1"
  },
  "peerDependencies": {
    "astro": ">=3.3.0"
  }
}
